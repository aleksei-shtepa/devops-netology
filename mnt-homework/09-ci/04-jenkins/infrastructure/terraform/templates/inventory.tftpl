---
all:
  hosts:
%{ for name, host in instances ~}
    ${name}-01:
      ansible_host: ${host.ip}
      ansible_user: ${host.user}
%{ endfor ~}
  children:
    jenkins:
      children:
        jenkins_masters:
          hosts:
            jenkins-master-01:
        jenkins_agents:
          hosts:
            jenkins-agent-01:
  vars:
    ansible_connection_type: paramiko
    ansible_user: centos
